{
  "name": "kenv",
  "version": "0.0.4",
  "description": "Load environment variables using jsonc.",
  "keywords": [
    "dotenv",
    "jsonc",
    "load",
    "environment"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "tsc --project tsconfig.lib.json",
    "build": "yarn clean && yarn build:lib",
    "clean": "rm -rf lib;",
    "update": "ncu -u",
    "prepublishOnly": "yarn test && yarn build",
    "test": "jest",
    "lint": "eslint . --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/igimanaloto/kenv"
  },
  "author": "Igi Manaloto",
  "license": "MIT",
  "homepage": "https://github.com/igimanaloto/kenv#readme",
  "dependencies": {
    "json5": "^2.1.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/json5": "^0.0.30",
    "@types/node": "^13.13.2",
    "eslint": "^6.8.0",
    "eslint-config-jnprt": "^0.0.9",
    "jest": "^25.3.0",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}

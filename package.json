{
  "name": "kenv",
  "version": "0.0.14",
  "description": "Load environment variables using jsonc.",
  "keywords": [
    "dotenv",
    "jsonc",
    "load",
    "environment"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "tsc --project tsconfig.lib.json",
    "build": "yarn clean && yarn build:lib",
    "clean": "rm -rf lib;",
    "update": "ncu -u",
    "prepublishOnly": "yarn test && yarn build",
    "test": "jest --forceExit",
    "lint": "eslint . --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/igimanaloto/kenv"
  },
  "author": "Igi Manaloto",
  "license": "MIT",
  "homepage": "https://github.com/igimanaloto/kenv#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "deep-freeze": "^0.0.1",
    "flat": "^5.0.2",
    "json5": "^2.1.3"
  },
  "devDependencies": {
    "@types/deep-freeze": "^0.1.2",
    "@types/flat": "^5.0.1",
    "@types/jest": "^26.0.15",
    "@types/json5": "^0.0.30",
    "@types/node": "^14.14.6",
    "eslint": "^7.12.1",
    "eslint-config-jnprt": "^0.0.11",
    "jest": "^26.6.3",
    "npm-check-updates": "^9.2.4",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
